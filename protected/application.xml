<?xml version="1.0" encoding="utf-8" ?>
<application ID="pastebin" mode="Debug">
    <paths>
        <using namespace="Application.controls.*" />
        <using namespace="System.I18N.*" />
        <using namespace="Application.PasteGlobalization" />
    </paths>
    <modules>
    <module id="globalization" class="PasteGlobalization" DefaultCulture="en_US">
        <translation type="XLIFF"
            source="Application.messages"
            autosave="true" cache="true" />
    </module>
    </modules>
</application>
